<%= form_for @grupo_tutor, url: grupo_tutores_path, html: {method: :post} do |f| %>
        <% if @grupo_tutor.errors.any? %>
               <%= pluralize(@grupo_tutor.errors.count, "error") %>

                <% @grupo_tutor.errors.full_messages.each do |actual| %>
                        <%= actual %>
                    <% end %>
            <% end %>
        <p>
        <%= f.label :tutor %><br>
        <%= f.select('estudiante_id', Estudiante.where(rol_id: (Rol.find_by_nombres('Tutor')).id).collect {|c| [c.nombres, c.id]}) %>
      </p>

        <%= render 'tutorados'%>

        <%= f.submit %>
    <% end %>