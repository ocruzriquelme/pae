<header class="header">
  <a href="#" class="logo">
    <!-- Add the class icon to your logo image or logo icon to add the margining -->
    PAE
  </a>
  <!-- Header Navbar: style can be found in header.less -->
  <nav class="navbar navbar-static-top" role="navigation">
    <!-- Sidebar toggle button-->
    <a href="#" class="navbar-btn sidebar-toggle" data-toggle="offcanvas" role="button">
      <span class="sr-only">Toggle navigation</span>
      <span class="icon-bar"></span>
      <span class="icon-bar"></span>
      <span class="icon-bar"></span>
    </a>
    <div class="navbar-right">
      <ul class="nav navbar-nav navbar-right">

        <!-- User Account: style can be found in dropdown.less -->

        <p class="navbar-text pull-right">
          <% if admin_signed_in? %>
              Conectado <strong><%= current_admin.email %></strong>.
              <%= link_to 'Editar Perfil', edit_admin_registration_path, :class => 'navbar-link' %> |
              <%= link_to "Cerrar Sesion", destroy_admin_session_path, method: :delete, :class => 'navbar-link'  %>
          <% else %>
              <%= link_to "Registrar", new_admin_registration_path, :class => 'navbar-link'  %> |
              <%= link_to "Iniciar Sesion", new_admin_session_path, :class => 'navbar-link'  %>
          <% end %>
        </p>

      </ul>
    </div>
  </nav>
</header>


